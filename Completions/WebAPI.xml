<?xml version="1.0" encoding="UTF-8"?>
<completions>
	<provider name="webapi.globals">
		<syntax>vue</syntax>
		<selector>*:not(string,comment)</selector>
		<expression>(?&lt;!\.)\b[a-zA-Z_][a-zA-Z0-9_]*</expression>
		
		<set>webapi.classes</set>
		<set>webapi.globals</set>
	</provider>
	
	<provider name="webapi.properties">
		<syntax>vue</syntax>
		<selector>*:not(string,comment)</selector>
		<expression>(?&lt;=\.)\b([a-zA-Z_][a-zA-Z0-9_]*)?</expression>
		
		<set>webapi.globals</set>
		<set>webapi.properties</set>
	</provider>
</completions>